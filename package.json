    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.4.0",
    "jest-dom": "^3.1.2",
    "jest-watch-typeahead": "^0.2.1",
    "prettier": "^1.16.4",
    "react-testing-library": "^6.0.0"
